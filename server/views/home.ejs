<!-- Partials -->
<%- include partials/header.ejs %> <%- include partials/main_nav.ejs %> <%-
include partials/content.ejs %>

<!-- Page Content -->
<div class="container">
  <div class="row">
    <!--   <div class="col-md-offset-1 col-md-10"> -->
    <div class="col-lg-offset-1 col-lg-10">
      <br />
      <div class="table-responsive">
        <table class="table table-bordered table-striped table-hover">
          <thead>
            <tr>
              <th class="text-center col-2">Title</th>
              <th class="text-center col-4">Description</th>
              <th class="text-center col-2">Due Date</th>
              <th class="text-center col-2">Survey status</th>
            </tr>
          </thead>
          <tbody>
            <!--The body of the table-->
            <% for(let count = 0; count < SurveyList.length; count++) { %>
            <tr>
              <td class="text-center col-2"><%= SurveyList[count].Title %></td>
              <td class="text-center col-4">
                <%= SurveyList[count].Description %>
              </td>
              <td class="text-center col-2">
                <%= SurveyList[count].DueDate %>
              </td>
              <td class="text-center col-2">
                <%= SurveyList[count].SurveyStatus %>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      <div class="col-lg-offset-1 col-lg-10 text-center">
        <ul class="pagination pagination-lg pager" id="myPager"></ul>
      </div>
    </div>
  </div>
</div>

<!-- Account for Fixed Bottom -->
<br />
<br />
<br />
<br />

<!-- Partials -->
<%- include partials/bottom_nav.ejs %> <%- include partials/footer.ejs %>
